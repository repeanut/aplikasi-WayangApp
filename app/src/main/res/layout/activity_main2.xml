<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/latarblkg"
    tools:context=".MainActivity5">

    <ImageView
        android:layout_width="119dp"
        android:layout_height="67dp"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_marginEnd="14dp"
        android:layout_marginBottom="698dp"
        android:src="@drawable/logoproject" />

    <ImageButton
        android:layout_width="313dp"
        android:layout_height="51dp"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_marginEnd="51dp"
        android:layout_marginBottom="102dp"
        android:background="@drawable/button"
        android:fontFamily="@font/m_plus_1p_bold"
        android:padding="10dp"
        android:visibility="visible" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_marginEnd="51dp"
        android:layout_marginBottom="113dp"
        android:fontFamily="@font/m_plus_1p_bold"
        android:text="Get Started"
        android:textColor="#FFF1F1"
        android:textSize="20dp"
        android:onClick="GetStarted"
        android:visibility="visible" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_marginEnd="0dp"
        android:layout_marginBottom="658dp"
        android:fontFamily="@font/lilita_one"
        android:padding="10dp"
        android:text="WAYANGAPP"
        android:textAlignment="center"
        android:textColor="@color/white"
        android:textSize="20dp"
        android:textStyle="bold" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="60dp"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_marginEnd="50dp"
        android:layout_marginBottom="614dp"
        android:fontFamily="@font/m_plus_1p_bold"
        android:padding="10dp"
        android:text="the story of Indonesian puppetry "
        android:textAlignment="center"
        android:textColor="@color/white"
        android:textSize="15dp"
        android:textStyle="bold" />

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="5dp"
        android:layout_marginTop="90dp"
        android:layout_marginEnd="14dp"
        android:layout_marginBottom="51dp"
        android:fontFamily="@font/m_plus_1p_bold"
        android:padding="10dp"
        android:text="Already have account? Log in"
        android:textAlignment="center"
        android:textColor="@color/white"
        android:textSize="15dp" />

</RelativeLayout>